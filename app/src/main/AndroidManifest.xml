<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.learnabc">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Score_board_hard"></activity>
        <activity android:name=".Score_board_medium" />
        <activity android:name=".Score_board_easy" />
        <activity android:name=".ScoreBoard_level" />
        <activity android:name=".Quite" />
        <activity android:name=".Test_medium" />
        <activity android:name=".Test_hard" />
        <activity android:name=".Test_easy" />
        <activity android:name=".Level" />
        <activity android:name=".WrongAnswer" />
        <activity android:name=".CorrectAnswer" />
        <activity android:name=".Result" />
        <activity android:name=".Learn" />
        <activity android:name=".Player" />
        <activity
            android:name=".SplashScreen"
            android:theme="@style/SplashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity"
            android:theme="@style/AppTheme" />
    </application>

</manifest>